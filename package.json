{
  "name": "prisma-vercel-graphql",
  "scripts": {
    "start": "node dist/graphql",
    "clean": "rm -rf dist",
    "build": "npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "GENERATE_ARTIFACTS=true ts-node --transpile-only api/_lib/schema",
    "dev": "GENERATE_ARTIFACTS=true ./node_modules/.bin/tsc-watch --onSuccess 'npx micro ./dist/index.js'"
  },
  "dependencies": {
    "@prisma/client": "2.14.0",
    "@types/mocha": "^8.2.0",
    "@types/node-fetch": "2.5.7",
    "@types/yup": "^0.29.11",
    "apollo-server-micro": "2.19.1",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie": "^0.4.1",
    "graphql": "15.4.0",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.2.1",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "0.28.0",
    "querystring": "^0.2.0",
    "ts-mocha": "^8.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@now/node": "1.8.5",
    "@prisma/cli": "2.14.0",
    "@types/cookie": "^0.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.19.13",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "tsc-watch": "4.2.9",
    "typescript": "4.1.3",
    "vercel": "21.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": "12.x"
  }
}
